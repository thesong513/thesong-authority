<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thesong.authority.mapper.PowerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.thesong.authority.entity.Power">
        <id column="power_id" property="powerId" />
        <result column="power_name" property="userName" />
        <result column="operation" property="password" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        power_id AS powerId, power_name AS powerName,operation
    </sql>


</mapper>
